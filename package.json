{
  "name": "dep-cli",
  "version": "1.0.0",
  "description": "A command-line tool for analyzing dependencies under node_moudles",
  "type": "module",
  "bin": {
    "analyze": "packages/cli/dist/bundle.es.js"
  },
  "scripts": {
    "dev": "node scripts/dev-web.js",
    "dev:web": "node scripts/dev-web.js",
    "dev:cli": "ts-node --esm packages/cli/lib/analyze.ts",
    "build:web": "vite build packages/web/ && vite preview packages/web/",
    "build:cli": "node scripts/build-cli.js --cjs --es",
    "build-cli-cjs": "node scripts/build-cli.js --cjs",
    "build-cli-es": "node scripts/build-cli.js --es",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lilming123/dep_cli.git"
  },
  "keywords": [
    "node_moudles",
    "cli"
  ],
  "author": "lilming",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lilming123/dep_cil/issues"
  },
  "homepage": "https://github.com/lilming123/dep_cil#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/node": "^20.4.5",
    "commander": "^11.0.0",
    "commitizen": "^4.3.0",
    "cz-git": "^1.7.0",
    "esbuild": "^0.18.17",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "minimist": "^1.2.8",
    "prettier": "^3.0.0",
    "rollup": "^3.26.3",
    "rollup-plugin-polyfill-node": "^0.12.0",
    "ts-node": "^10.9.1",
    "tslib": "^2.6.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.5"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  }
}
