{
  "name": "truth-cli",
  "version": "1.0.2",
  "description": "A command-line tool for analyzing dependencies under node_moudles",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "truth-cli": "dist/index.js"
  },
  "scripts": {
    "init": "pnpm install",
    "test": "ts-node --esm lib/genPkgs.ts",
    "dev": "ts-node --esm scripts/dev-web.ts",
    "web": "cd packages/web && pnpm run dev",
    "dev:cli": "ts-node --esm /dist/bundle.es.js",
    "build:web": "ts-node --esm scripts/build-charts.ts",
    "build:cli": "ts-node --esm scripts/build-cli.ts --cjs --es",
    "build-cli-cjs": "nts-ode --esm scripts/build-cli.ts --cjs",
    "build-cli-es": "ts-node --esm scripts/build-cli.ts --es",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lilming123/dep_cli.git"
  },
  "keywords": [
    "node_moudles",
    "cli",
    "node"
  ],
  "author": "lilming",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lilming123/dep_cil/issues"
  },
  "homepage": "https://github.com/lilming123/dep_cil#readme",
  "dependencies": {
    "commander": "^11.0.0",
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.8",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@rollup/plugin-commonjs": "^25.0.3",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/express": "^4.17.17",
    "@types/minimist": "^1.2.2",
    "chalk": "^5.3.0",
    "commander": "^11.0.0",
    "commitizen": "^4.3.0",
    "cz-git": "^1.7.0",
    "eslint": "^8.45.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "local-pkg": "^0.4.3",
    "minimist": "^1.2.8",
    "ora": "^6.3.1",
    "prettier": "^3.0.0",
    "rollup": "^3.26.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.6"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": true
    }
  }
}
